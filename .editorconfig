root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.{c,h}]
indent_style = space
indent_size = 4
max_line_length = 120

[*.md]
trim_trailing_whitespace = false

[CMakeLists.txt]
indent_style = space
indent_size = 4

[*.sh]
indent_style = space
indent_size = 4
